<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>

<VBox alignment="CENTER" prefHeight="672.0" prefWidth="344.0" stylesheets="@stylecomb.css" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ec.edu.uees.proyectocircular.SolicitarTurnosVista">
   <children>
      <VBox alignment="CENTER" fillWidth="false" prefHeight="310.0" prefWidth="344.0" spacing="20.0" VBox.vgrow="ALWAYS">
         <children>
            <Label alignment="TOP_LEFT" prefHeight="30.0" prefWidth="138.0" text="Número de Ticket:" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="-20.0" left="-70.0" top="10.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="txtcodigo" editable="false" focusTraversable="false" prefHeight="26.0" prefWidth="220.0" promptText="Numero de ticket" VBox.vgrow="NEVER" />
            <TextField fx:id="txtnombre" prefHeight="26.0" prefWidth="220.0" promptText="Ingrese su nombre" VBox.vgrow="NEVER" />
            <TextField fx:id="txtapellido" prefHeight="26.0" prefWidth="220.0" promptText="Ingrese su apellido" VBox.vgrow="NEVER">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </TextField>
            <Button fx:id="btnTurno" mnemonicParsing="false" onAction="#crearEdificio" text="Crear Turno">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin></Button>
         </children>
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
      </VBox>
      <VBox alignment="CENTER" prefHeight="366.0" prefWidth="344.0" spacing="20.0" stylesheets="@stylecomb.css" VBox.vgrow="ALWAYS">
         <children>
            <Label text="Síntoma:" />
            <Group VBox.vgrow="ALWAYS">
               <children>
                  <Rectangle fx:id="myRectangle" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="40.0" stroke="BLACK" strokeType="INSIDE" width="304.0" />
                  <ComboBox fx:id="cbo1" prefHeight="26.0" prefWidth="304.0" promptText="CRITÍCO" />
               </children>
            </Group>
            <Group>
               <children>
                  <Rectangle fx:id="myRectangle1" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="40.0" stroke="BLACK" strokeType="INSIDE" width="304.0" />
                  <ComboBox fx:id="cbo2" prefHeight="26.0" prefWidth="304.0" promptText="EMERGENCIA" />
               </children>
            </Group>
            <Group VBox.vgrow="ALWAYS">
               <children>
                  <Rectangle fx:id="myRectangle11" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="40.0" stroke="BLACK" strokeType="INSIDE" width="304.0" />
                  <ComboBox fx:id="cbo3" prefHeight="26.0" prefWidth="304.0" promptText="URGENCIA" />
               </children>
            </Group>
            <Group VBox.vgrow="ALWAYS">
               <children>
                  <Rectangle fx:id="myRectangle111" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="40.0" stroke="BLACK" strokeType="INSIDE" width="304.0" />
                  <ComboBox fx:id="cbo4" prefHeight="26.0" prefWidth="304.0" promptText="URGENCIA MENOR" />
               </children>
            </Group>
            <Group VBox.vgrow="ALWAYS">
               <children>
                  <Rectangle fx:id="myRectangle1111" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="40.0" stroke="BLACK" strokeType="INSIDE" width="304.0" />
                  <ComboBox fx:id="cbo5" prefHeight="26.0" prefWidth="304.0" promptText="NO URGENTE" />
               </children>
            </Group>
         </children>
         <padding>
            <Insets bottom="30.0" left="20.0" right="20.0" />
         </padding>
      </VBox>
   </children>
</VBox>
